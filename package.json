{
  "name": "@rqt/namecheap-web",
  "version": "1.2.0",
  "description": "An API to namecheap.com via the web interface, with an ability to log in using 2-factor Auth and check Whois.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec -t 20000",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "b": "alamode src -o build -s",
    "d": "NODE_DEBUG=doc doc src/index.js -g",
    "build": "yarn-s d b doc",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "example/whois.js": "yarn e example/whois.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/namecheap-web.git"
  },
  "keywords": [
    "namecheap-web",
    "rqt",
    "domains",
    "domain",
    "namecheap",
    "api",
    "web"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/namecheap-web/issues"
  },
  "homepage": "https://github.com/rqt/namecheap-web#readme",
  "devDependencies": {
    "alamode": "1.5.1",
    "documentary": "1.20.1",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "3.5.1"
  },
  "dependencies": {
    "@rqt/ip": "1.0.0",
    "bosom": "1.0.0",
    "erte": "1.1.4",
    "mismatch": "1.0.3",
    "reloquent": "1.0.6",
    "rqt": "3.0.0"
  }
}
