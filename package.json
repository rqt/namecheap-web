{
  "name": "@rqt/namecheap-web",
  "version": "1.3.1",
  "description": "An API To Namecheap.com Via The Web Interface, With An Ability To Log In Using 2-Factor Auth To Whitelist IPs And Check Whois And Coupons.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec -t 30000",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "typal types/index.js src/index.js -c -t types/options.xml,types/ips.xml",
    "build": "yarn-s d b doc",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "e": "node example",
    "externs": "typal types/externs.js",
    "example/": "yarn e example/example.js",
    "example/whois.js": "yarn e example/whois.js"
  },
  "files": [
    "build",
    "types"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/namecheap-web.git"
  },
  "keywords": [
    "namecheap-web",
    "rqt",
    "domains",
    "domain",
    "namecheap",
    "api",
    "web"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/namecheap-web/issues"
  },
  "homepage": "https://github.com/rqt/namecheap-web#readme",
  "devDependencies": {
    "alamode": "^2.3.6",
    "documentary": "^1.29.0",
    "eslint-config-artdeco": "^1.0.1",
    "yarn-s": "^1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "@rqt/ip": "^1.1.0",
    "bosom": "^1.0.0",
    "erte": "^1.2.0",
    "mismatch": "^1.2.0",
    "reloquent": "^1.3.1",
    "rqt": "^3.1.4"
  }
}
